{
  "version": 2,
  "name": "RedisRateLimitingPython",
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "25mb" }
    },{
      "src": "now_build_staticfiles.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "server/static_root"
      }
    }
  ],
  "routes": [{
    "src": "/api/(.*)",
    "dest": "index.py"
}]
}
